%token Number<int> LParen RParen Comma Star;
%namespace calc;
%dont_use_stl;
%recover error;

List<int> : [PackList] LParen ListItems(0) RParen
	  | [PackListError] LParen error RParen
	  ;

ListItems<int>
          : [MakeList] Number(0)
          | [AddToList] ListItems(0) Comma Number(1)
          ;
